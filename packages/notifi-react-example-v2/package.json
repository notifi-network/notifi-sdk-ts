{
  "name": "@notifi-network/notifi-react-example-v2",
  "version": "6.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:next": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "test": "npm run test:card && npm run test:smartlink",
    "test:card": "cypress run --component --spec 'cypress/component/NotifiCardModal.cy.tsx' --browser chrome",
    "test:smartlink": "cypress run --component --spec 'cypress/component/NotifiSmartLink.cy.tsx' --browser chrome",
    "cypress:open": "cypress open",
    "cypress:ci": "npm run cypress:ci-card && npm run cypress:ci-smartlink",
    "cypress:ci-card": "cypress run --component --group notifi-e2e --spec 'cypress/component/NotifiCardModal.cy.tsx' --browser chrome --record --parallel",
    "cypress:ci-smartlink": "cypress run --component --group notifi-e2e --spec 'cypress/component/NotifiSmartLink.cy.tsx' --browser chrome --record --parallel"
  },
  "dependencies": {
    "@ethersproject/bytes": "^5.8.0",
    "@notifi-network/notifi-react": "^6.0.1",
    "@notifi-network/notifi-react-wallet-target-plugin": "^6.0.1",
    "@notifi-network/notifi-solana-hw-login": "^6.0.1",
    "@notifi-network/notifi-wallet-provider": "^6.0.1",
    "bip39": "^3.1.0",
    "ethers": "^6.11.1",
    "jwt-decode": "^4.0.0",
    "next": "^14.2.7",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "copy-webpack-plugin": "^12.0.2",
    "crypto-browserify": "^3.12.0",
    "cypress": "^13.11.0",
    "ethereumjs-wallet": "^1.0.2"
  }
}
